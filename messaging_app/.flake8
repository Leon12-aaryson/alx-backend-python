[flake8]
max-line-length = 127
max-complexity = 10
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    migrations,
    staticfiles,
    media,
    node_modules,
    .tox,
    .eggs,
    *.egg,
    build,
    dist
ignore = 
    E203,
    W503,
    E501,
    F401,
    F403,
    F405
per-file-ignores =
    __init__.py:F401
    migrations/*:E501
